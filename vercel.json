{
  "builds": [
    {
      "src": "src/main/server.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*.ts": {
      "runtime": "vercel-node@18.x",
      "handler": "dist/main/server.js"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}
